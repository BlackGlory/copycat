{
  "compilerOptions": {
    "target": "ES2018"
  , "module": "ES2015"
  , "moduleResolution": "Node"
  , "jsx": "react-jsx"
  , "allowSyntheticDefaultImports": true
  , "esModuleInterop": true
  , "strict": true
  , "removeComments": true
  , "noUnusedLocals": false
  , "noUnusedParameters": false
  , "baseUrl": "."
  , "paths": {
      "@src/*": ["src/*"]
    , "@components/*": ["src/components/*"]
    , "@converters/*": ["src/utils/converters/*"]
    , "@hooks/*": ["src/hooks/*"]
    , "@utils/*": ["src/utils/*"]
    }
  }
, "include": [
    "src"
  , "__tests__"
  ]
}
